<%= render :partial => 'admin/shared/configuration_menu' %>

<h1><%= t(:ext_my_news_new_item) %></h1>
<%= render "shared/error_messages", :target => @newsitem %>

<% form_for(:newsitem, :url => collection_url) do |f| %>
  <%= render :partial => "form", :locals => { :f => f } %>
  <p class="form-buttons">
    <%= button t("actions.create"), nil, 'submit', {"class" => "wymupdate"} %>
    <%= t("or") %> <%= link_to t("actions.cancel"), collection_url %>
  </p>
<% end %>

