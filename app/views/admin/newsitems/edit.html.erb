<%= render :partial => 'admin/shared/configuration_menu' %>

<h1><%= t("ext_my_news_editing_item") %></h1>
<%= render "shared/error_messages", :target => @newsitem %>

<% form_for(:newsitem, :url => object_url, :html => { :method => :put }) do |f| -%>
  <%= render :partial => "form", :locals => { :f => f } %>
  <p class="form-buttons">
    <%= button t("actions.update"), nil, 'submit', {"class" => "wymupdate"} %>
    <%= t("or") %> <%= link_to t("actions.cancel"), collection_url %>
  </p>
<% end %>
